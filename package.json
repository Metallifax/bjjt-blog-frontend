{
  "name": "blog-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@uiw/react-md-editor": "^3.11.2",
    "bootstrap": "^5.1.3",
    "history": "^5.3.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-bootstrap": "^0.26.1",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
    "redux-mock-store": "^1.5.4",
    "rehype-sanitize": "^5.0.1",
    "sass": "^1.49.9",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-all": "npm run format && npm run lint && npm test -- --watchAll=false && npm run cy:sr",
    "test-cov": "npm test -- --watchAll=false --coverage",
    "eject": "react-scripts eject",
    "format": "prettier -c src && prettier -c cypress",
    "format-write": "prettier --write src && prettier --write cypress",
    "lint": "eslint src/ && eslint cypress/",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:sr": "start-test 3000 cy:run",
    "cy:so": "start-test 3000 cy:open"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.jsx"
    ],
    "transformIgnorePatterns": [
      "//node_modules/(?!uri-transformer)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^9.5.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "prettier": "^2.6.1",
    "start-server-and-test": "^1.14.0"
  }
}
